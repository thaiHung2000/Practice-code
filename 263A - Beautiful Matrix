#include <stdio.h>
int main()
{
    int a;
    int m = 0, n = 0;
    for(int i = 1 ; i <=5 ; i++)
    {
        for(int j = 1; j <= 5; j++ )
        {
            scanf("%d", &a);
            if( a == 1 )
            {
                m = i ;
                n = j;
                break;
 
            }
        }
    }
    if( m >=3 )
    {
        m = m -3 ;
    }
    else
    {
        m = 3 - m;
    }
    if( n >=3 )
    {
        n = n -3 ;
    }
    else
    {
        n = 3 - n;
    }
    int moves = m + n;
    printf("%d", moves);
 
 
}
